<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.unicef.repository.donate.DonateAreaRepository">
    <insert id="donateAreaInsert"
            parameterType="com.unicef.domain.DonateArea"
            useGeneratedKeys="true"
            keyProperty="donate_area_id"
    >
        insert into
        donate_mst
        values(
        0,
        #{donate_name},
        #{donate_area}
        now(),
        now()
        )
    </insert>
</mapper>