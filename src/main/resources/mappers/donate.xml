<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.unicef.repository.donate.DonateRepository">
<!--table4 후원분야-->
    <insert id="donateAreaInsert"
            parameterType="com.unicef.domain.DonateArea"
            useGeneratedKeys="true"
            keyProperty="donate_area_id"
    >
        insert into
        donate_area_mst(
        donate_area_id,
        donate_name,
        donate_area,
        create_date,
        update_date
        )
        values(
        0,
        #{donate_name},
        #{donate_area},
        now(),
        now()
        )
    </insert>
<!--table5 후원-->
    <insert id="donateInsert"
            parameterType="com.unicef.domain.Donate"
            useGeneratedKeys="true"
            keyProperty="donate_id"
    >
        insert into
        donate_mst(
            donate_id,
            donate_type,
            donate_amount,
            donate_area_id,
            create_date,
            update_date
        )
        values(
            0,
            #{donate_type},
            #{donate_amount},
            #{donate_area_id},
            now(),
            now()
        )
    </insert>
</mapper>